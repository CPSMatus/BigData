
BEGIN
  FOR r IN (
            SELECT SUBTIPO_TRABAJO_CANCHA_ID FROM AF_SUBTIPO_TRABAJO_CANCHA
            WHERE
             InStr(':'|| :P7_SUBTIPO_TRABAJO_CANCHA || ':', ':'||SUBTIPO_TRABAJO_CANCHA_ID || ':') > 0
              AND SUBTIPO_TRABAJO_CANCHA_ID IS NOT NULL)
  LOOP
    insert into AF_LESION_TRABAJO_CANCHA(ID_SESION_POST_LESION,ID_SUBTIPO_TRABAJO_CANCHA)values(:P7_ID_SESION_POST_LESION,r.SUBTIPO_TRABAJO_CANCHA_ID);
  END LOOP;

  delete from AF_LESION_TRABAJO_CANCHA where ID_SUBTIPO_TRABAJO_CANCHA is null;
END;






BEGIN
  FOR r IN (
            SELECT SUBTIPO_TRABAJO_FUERZA_ID FROM AF_SUBTIPO_TRABAJO_FUERZA
            WHERE
             InStr(':'|| :P13_FUERZA_ANALITICA || ':', ':'||SUBTIPO_TRABAJO_FUERZA_ID || ':') > 0
              AND SUBTIPO_TRABAJO_FUERZA_ID IS NOT NULL)

LOOP
    insert into AF_LESION_TERAPIA_FUERZA(ID_SESION_POST_LESION,ID_TIPO_FORTALECIMIENTO,ID_SUBTIPO_TRABAJO_FUERZA)values(:P13_ID_SESION_POST_LESION,:P13_ID_TIPO_FORTALECIMIENTO,r.SUBTIPO_TRABAJO_FUERZA_ID);
  END LOOP;

  FOR r IN (
            SELECT SUBTIPO_TRABAJO_FUERZA_ID FROM AF_SUBTIPO_TRABAJO_FUERZA
            WHERE
             InStr(':'|| :P13_FUERZA_LIGA_RESISTENCIA || ':', ':'||SUBTIPO_TRABAJO_FUERZA_ID || ':') > 0
              AND SUBTIPO_TRABAJO_FUERZA_ID IS NOT NULL)

  LOOP
    insert into AF_LESION_TERAPIA_FUERZA(ID_SESION_POST_LESION,ID_TIPO_FORTALECIMIENTO,ID_SUBTIPO_TRABAJO_FUERZA)values(:P13_ID_SESION_POST_LESION,:P13_ID_TIPO_FORTALECIMIENTO,r.SUBTIPO_TRABAJO_FUERZA_ID);
  END LOOP;


  delete from AF_LESION_TERAPIA_FUERZA where ID_SUBTIPO_TRABAJO_FUERZA is null;
END;































BEGIN
    if ((:P11_ID_SEGMENTO_CORPORAL > 0 ) and (:P11_ID_STC > 0)) THEN
        insert into AF_LESION_TERAPIA_CONSULTORIO(ID_SESION_POST_LESION,ID_GRUPO_MUSCULAR,ID_SUBTIPO_TERAPIA_CONSULTORIO)values(:P11_ID_SESION_POST_LESION,:P11_ID_SEGMENTO_CORPORAL,:P11_ID_STC);
    end if;

    if ((:P11_SEGMENTO1_CORPORAL > 0 ) and (:P11_S1TC > 0) ) THEN
        insert into AF_LESION_TERAPIA_CONSULTORIO(ID_SESION_POST_LESION,ID_GRUPO_MUSCULAR,ID_SUBTIPO_TERAPIA_CONSULTORIO)values(:P11_ID_SESION_POST_LESION,:P11_SEGMENTO1_CORPORAL,:P11_S1TC);
    end if;


    if ((:P11_SEGMENTO2_CORPORAL > 0 ) and (:P11_S2TC > 0) ) THEN
        insert into AF_LESION_TERAPIA_CONSULTORIO(ID_SESION_POST_LESION,ID_GRUPO_MUSCULAR,ID_SUBTIPO_TERAPIA_CONSULTORIO)values(:P11_ID_SESION_POST_LESION,:P11_SEGMENTO2_CORPORAL,:P11_S2TC);
    end if;


    if ((:P11_SEGMENTO3_CORPORAL > 0 ) and (:P11_S3TC > 0) ) THEN
        insert into AF_LESION_TERAPIA_CONSULTORIO(ID_SESION_POST_LESION,ID_GRUPO_MUSCULAR,ID_SUBTIPO_TERAPIA_CONSULTORIO)values(:P11_ID_SESION_POST_LESION,:P11_SEGMENTO3_CORPORAL,:P11_S3TC);
    end if;

    if ((:P11_SEGMENTO4_CORPORAL > 0 ) and (:P11_S4TC > 0) ) THEN
        insert into AF_LESION_TERAPIA_CONSULTORIO(ID_SESION_POST_LESION,ID_GRUPO_MUSCULAR,ID_SUBTIPO_TERAPIA_CONSULTORIO)values(:P11_ID_SESION_POST_LESION,:P11_SEGMENTO4_CORPORAL,:P11_S4TC);
    end if;


    if ((:P11_SEGMENTO5_CORPORAL > 0 ) and (:P11_S5TC > 0) ) THEN
        insert into AF_LESION_TERAPIA_CONSULTORIO(ID_SESION_POST_LESION,ID_GRUPO_MUSCULAR,ID_SUBTIPO_TERAPIA_CONSULTORIO)values(:P11_ID_SESION_POST_LESION,:P11_SEGMENTO5_CORPORAL,:P11_S5TC);
    end if;

    if ((:P11_SEGMENTO6_CORPORAL > 0 ) and (:P11_S6TC > 0) ) THEN
        insert into AF_LESION_TERAPIA_CONSULTORIO(ID_SESION_POST_LESION,ID_GRUPO_MUSCULAR,ID_SUBTIPO_TERAPIA_CONSULTORIO)values(:P11_ID_SESION_POST_LESION,:P11_SEGMENTO6_CORPORAL,:P11_S6TC);
    end if;

    if ((:P11_SEGMENTO7_CORPORAL > 0 ) and (:P11_S7TC > 0) ) THEN
        insert into AF_LESION_TERAPIA_CONSULTORIO(ID_SESION_POST_LESION,ID_GRUPO_MUSCULAR,ID_SUBTIPO_TERAPIA_CONSULTORIO)values(:P11_ID_SESION_POST_LESION,:P11_SEGMENTO7_CORPORAL,:P11_S7TC);
    end if;

    if ((:P11_SEGMENTO8_CORPORAL > 0 ) and (:P11_S8TC > 0) ) THEN
        insert into AF_LESION_TERAPIA_CONSULTORIO(ID_SESION_POST_LESION,ID_GRUPO_MUSCULAR,ID_SUBTIPO_TERAPIA_CONSULTORIO)values(:P11_ID_SESION_POST_LESION,:P11_SEGMENTO8_CORPORAL,:P11_S8TC);
    end if;

    if ((:P11_SEGMENTO9_CORPORAL > 0 ) and (:P11_S9TC > 0) ) THEN
        insert into AF_LESION_TERAPIA_CONSULTORIO(ID_SESION_POST_LESION,ID_GRUPO_MUSCULAR,ID_SUBTIPO_TERAPIA_CONSULTORIO)values(:P11_ID_SESION_POST_LESION,:P11_SEGMENTO9_CORPORAL,:P11_S9TC);
    end if;
END;
